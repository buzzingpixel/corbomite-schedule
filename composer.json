{
    "name": "buzzingpixel/corbomite-schedule",
    "version": "1.0.1",
    "description": "Corbomite Schedule",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-migrations": "^1.0",
        "buzzingpixel/corbomite-db": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\schedule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "corbomite\\tests\\": "tests/"
        }
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "cliActionConfigFilePath": "src/actionConfig.php"
    }
}
